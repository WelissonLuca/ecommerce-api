{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
         "prettier"
    ],
    "plugins": [
  "prettier"
],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-tabs":"off",
        "camelcase":"warn",
        "no-console": "off",
         "prettier/prettier": "off"
    }
}
